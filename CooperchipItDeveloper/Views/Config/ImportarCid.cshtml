@{
    ViewData["Title"] = "Medicl Menagement Sys - Upload and Save Csv Cid";
}

<div class="row" style="margin-top: 12px;">
    <div class="col-lg-12">
        <p>@Html.ActionLink("Home", "Index", "Home", null, new { @class = "btn btn-default" })</p>

        <div class="progress-bar-animated fadeInRightBig">
            <div class="panel panel-primary toggle">
                <div class="panel-heading">
                    <h4 class="panel-title"><i class="fa fa-magic"></i> @ViewData["Title"]</h4>
                </div>
                <div class="panel-body">

                    <form asp-controller="Cid" asp-action="ImportCid"
                        name="ImportCid" method="post"
                        enctype="multipart/form-data">
                        
                        <input type="file" name="file" /> <hr />

                        <input type="submit" name="submit" id="submitCid" value="Carregar..." class="btn btn-info" />

                    </form>
                </div>
            </div>
        </div>
    </div>
</div>
